{
	"type": "commonjs",
	"name": "jab-express",
	"version": "1.0.0",
	"scripts": {
		"start": "ts-node src/index.ts",
		"dev": "cross-env NODE_ENV=development nodemon src/index.ts",
		"build": "rimraf dist && tsc",
		"lint": "eslint \"src/**/*.{ts,tsx,js}\" \"db/**/*.ts\" --fix",
		"serve": "node dist/index.js",
		"format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
		"db:push": "drizzle-kit push --config=drizzle.config.ts",
		"db:generate": "drizzle-kit generate --config=drizzle.config.ts",
		"db:studio": "drizzle-kit studio",
		"db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
		"db:seed": "tsx ./db/seed.ts",
		"db:reset": "tsx ./db/reset.ts",
		"openapi:generate": "npm run build && node scripts/generate-openapi.js",
		"openapi:validate": "npm run openapi:generate",
		"types:generate": "npm run openapi:generate",
		"generate:types": "node ../scripts/generate-all-types.js"
	},
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@profiller-hr/shared-types": "file:../shared-types",
		"@ai-sdk/openai": "^1.3.22",
		"@types/jsonwebtoken": "^9.0.10",
		"ai": "^4.3.16",
		"axios": "^1.10.0",
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"drizzle-kit": "^0.31.1",
		"drizzle-orm": "^0.43.1",
		"drizzle-zod": "^0.7.1",
		"express": "^4.21.0",
		"express-rate-limit": "^7.5.0",
		"express-slow-down": "^2.1.0",
		"express-validator": "^7.2.1",
		"helmet": "^8.1.0",
		"inversify": "^7.5.2",
		"jose": "^6.0.11",
		"jsonwebtoken": "^9.0.2",
		"lodash.merge": "^4.6.2",
		"morgan": "^1.10.0",
		"next-auth": "^4.24.11",
		"nodemailer": "^6.9.15",
		"pg": "^8.16.0",
		"pg-hstore": "^2.3.4",
		"prom-client": "^15.1.3",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.0.1",
		"sequelize": "^6.37.3",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.2",
		"zod": "^3.24.4"
	},
	"devDependencies": {
		"@eslint/js": "^9.27.0",
		"@faker-js/faker": "^9.8.0",
		"@types/bcrypt": "^5.0.2",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/lodash.merge": "^4.6.9",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.15.18",
		"@types/nodemailer": "^6.4.16",
		"@types/pg": "^8.15.2",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"cross-env": "^7.0.3",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"globals": "^16.1.0",
		"i": "^0.3.7",
		"nodemon": "^3.1.7",
		"npm": "^11.4.0",
		"prettier": "^3.5.3",
		"tsx": "^4.19.4",
		"typescript-eslint": "^8.32.1"
	}
}
